/**main.wxss**/

.userinfo {
  display: flex;
  align-items: center;
}

.img {
  width: 20px;
  height: 20px;
  padding-right: 5px;
}

.span {
  width: 20px;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 5;
}

.userinfo-nickname {
  color: #aaa;
}

.cash-box {
  height: 20%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 50rpx 0;
  box-sizing: border-box;
}

.title {
  display: flex;
  height: 20px;
  background-color:#aaa;
  align-items: center;
  align-content: center;
  padding: 5px;
}

.cash {
  font-size: 50px;
}

.usermotto {
  margin-top: 100px;
}

.flex-wrp {
  display: flex;
}

.zb {
  display: flex;
  flex-direction: column;
  align-items: center;
}

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 26px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

